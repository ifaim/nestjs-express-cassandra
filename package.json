  },
  "devDependencies": {
    "@nestjs/common": "^8.2.4",
    "@nestjs/core": "^8.2.4",
    "@types/node": "10.12.18",
    "@types/uuid": "^8.3.3",
    "husky": "3.0.9",
    "lint-staged": "^12.1.4",
    "prettier": "1.18.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "^7.1.0",
    "tslint": "5.20.0",
